---
interface Props {
	title: string;
}

const { title } = Astro.props;

import Header from "../components/Generics/Header.astro"
import Turno from "../components/Generics/Turno.astro";
import Footer from "../components/Generics/Footer.astro";
---

<!doctype html>
<html lang="es">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Astro description" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/ico" href="img/foto-principal.ico" />
		<meta name="generator" content={Astro.generator} />
		<title>Josefina Videau - {title}</title>

		<!-- Open Graph -->
		<meta property="og:title" content={`Josefina Videau - ${title}`} />
		<meta property="og:description" content="PÃ¡gina Web Lic. en Nutricion Josefina Videau" />
		<meta property="og:image" content="/img/main-section/foto-principal.jpg" />
		<meta property="og:url" content={Astro.url} />
		<meta property="og:type" content="website" />
	</head>
	<body>
		<Header />
		<Turno />
		<slot />
		<Footer />
	</body>
</html>

